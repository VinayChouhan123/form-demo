<h1>Users Lists</h1>

<table width="30%">
	<thead>
		<tr>
			<th>Name</th>
			<th>Email</th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.name %></td>
				<td><%= user.email %></td>
			</tr>
		<% end %>
	</tbody>
</table><br>

<%= link_to "Create User", new_user_path(@user) %>
